package test.ch03;

public class CompareOperation {

	public static void main(String[] args) {
		int n1 = 10;
		int n2 = 10;		
		boolean result1 = (n1 <= n2);
		System.out.println("resuxlt1: " + result1);
		
		char ch1 = 'A';
		char ch2 = 'B';
		boolean result2 = (ch1 < ch2);
		System.out.println("result2: " + result2);
		
		
		
		
	}

}
