package test.ch01;

public class Hello {

	public static void main(String[] args) {
//		System.out.println("Hello Java");

//		int a = 10, b = 12;
//		int result = a + b;
		int num1 = 0b1011;	//2진수 
		int num2 = 0206;	//8진수
		int num3 = 365;		//10진수
		int num4 = 0xB3;	//16진수

		System.out.println(num1);
		System.out.println(num2);
		System.out.println(num3);
		System.out.println(num4);
	}

}