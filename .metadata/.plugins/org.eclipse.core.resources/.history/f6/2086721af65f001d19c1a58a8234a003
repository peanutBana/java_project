package test.ch03;

public class If_Ex {

	public static void main(String[] args) {
		int score = 85;
		if(score >= 90) {
			System.out.println("90점 이상입니다.");
		}
		else if(score >= 80){
			System.out.println("80점 이상입니다.");			
		}
		else if(score >= 70){
			System.out.println("70점 이상입니다.");			
		}
		else {
			System.out.println("70점 미만입니다.");
		}
	}

}
